int totalRows = ExcelReader.getTotalExcelRowsWithoutHeader(excelPath, excelSheet);
testRunner.testCase.setPropertyValue("TotalRows",totalRows.toString())

if(totalRows &lt; count) {
	execCompleted = true
}

if(execCompleted == false) {
	def headerRows = ExcelReader.readExcelHeader(excelPath, excelSheet);
	def actualData = ExcelReader.readExcelData(excelPath, excelSheet, count);	

	for(i = 0; i &lt; headerRows.size(); i++){
		for(j = 0; j &lt;= i; j++){
			testRunner.testCase.setPropertyValue(headerRows[i],actualData[j])
			dataAdded = true 
		}
	}
	setCount(count)

	if(dataAdded == true) {
		log.info "Data Added Successfully"
	} else {
		log.info "Data Addition Failed"
	}
} else {
	log.info "Execution Finished"
	testRunner.runTestStepByName("End")
	setCount(count)
}

def setCount(count) {
	count = count + 1
	testRunner.testCase.setPropertyValue("Count", count.toString())
}</script></con:config></con:testStep><con:testStep type="request" id="11879cd7-f602-43fc-befb-94a2ed26d243" name="add"><con:settings/><con:config xsi:type="con:RequestStep" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"><con:interface>ICalculator</con:interface><con:operation>add</con:operation><con:request name="add" id="75780792-5a9f-4535-9abe-5c653cb762f0"><con:settings><con:setting id="com.eviware.soapui.impl.wsdl.WsdlRequest@request-headers">&lt;xml-fragment/></con:setting></con:settings><con:encoding>UTF-8</con:encoding><con:endpoint>http://localhost:8088/mockICalculator</con:endpoint><con:request><![CDATA[<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:cal="http://www.parasoft.com/wsdl/calculator/">
